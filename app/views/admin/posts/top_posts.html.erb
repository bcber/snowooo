<table class="table">
  <thead>
  <tr>
    <th>#</th>
    <th>标题</th>
    <th>操作</th>
  </tr>
  </thead>

  <tbody>

  <% @posts.each.with_index(1) do |post,idx| %>
    <tr>
      <td><%= idx %></td>
      <td><%= link_to post.title, post %></td>
      <td>
        <%= link_to '置顶' , up_admin_post_path(post), class:"btn btn-default btn-xs"%>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>