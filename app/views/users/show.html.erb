      <th><%= @user.name  if @user.name %></th>
    </tr>
    <tr>
      <th>reset_password_send_at</th>
      <th><%= @user.reset_password_sent_at if @user.reset_password_sent_at %></th>
    </tr>
    <tr>
      <th>remember_created_at</th>
      <th><%= @user.remember_created_at if @user.remember_created_at %></th>
    </tr>
    <tr>
      <th>sign_in_count</th>
      <th><%= @user.sign_in_count if @user.sign_in_count %></th>
    </tr>
    
    <tr>
      <th>current_sign_in_at</th>
      <th><%= @user.current_sign_in_at if @user.current_sign_in_at %></th>
